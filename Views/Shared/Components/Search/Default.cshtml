
@model IEnumerable<String>



<div class="container-fluid bg-primary mb-5 wow fadeIn" data-wow-delay="0.1s" style="padding: 35px;">
    <div class="container">
        <form asp-action="Search" asp-controller="Home" method="get">
        <div class="row g-2">
            <div class="col-md-10">
                <div class="row g-2">
                    <div class="col-md-4">
                        
                        <input type="text" class="form-control border-0 py-3" placeholder="Search Keyword" name="input">
                    </div>
                    <div class="col-md-4">
                        <select class="form-select border-0 py-3" name = "type">
                            <option value="null">Property Type</option>
                            @foreach(var a in ViewBag.TypeList)
                            {
                                <option value="@a.Name">@a.Name</option>
                            }
                        </select>
                    </div>
                    <div class="col-md-4" >
                        <select class="form-select border-0 py-3" name="address">
                            <option value="null">Location</option>
                                
                                @foreach (var b in Model)
                                {
                                    <option value="@b">@b</option>
                                }

                                }
                        </select>
                    </div>
                </div>
                </div><input type="hidden" name="id" value="1" />
            <div class="col-md-2">
                <button class="btn btn-dark border-0 w-100 py-3">Search</button>
            </div>
            </div>
        </form>
    </div>
</div>
